<div class="container">
    <!--
    <div class="page-header" id="banner">
        
    </div>
    -->
    <h4>Matches</h4>
    <input id="systemCode" type="hidden" value="<%= @model.systemCode %>">
    <input id="id" type="hidden" value="<%= @model.id %>">
    <input id="id2" type="hidden" value="<%= @model2.id %>">
    <div class="row">
        <div class="col-lg-6">
            <h5><%= @model.name %>'s matches with <%= @model2.name %></h5>
        </div>
    </div>
    <div class="progress progress-striped active">
        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="0" aria-valuemin="0" aria-valuemax="100">
        </div>
        <div class="progress-value">Loading</div>
    </div>
    <div class="row">
        <div class="col-lg-6">
            <div id="matchCount"></div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-6">
            <div id="matches" class="panel-group"></div>
        </div>
    </div>
</div>